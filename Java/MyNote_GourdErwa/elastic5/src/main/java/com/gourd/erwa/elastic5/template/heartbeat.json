{
  "template": "*heartbeat",
  "order": 1,
  "settings": {
    "index": {
      "refresh_interval": "30s",
      "number_of_shards": 2,
      "number_of_replicas": 0
    }
  },
  "mappings": {
    "heartbeat": {
      "properties": {
        "clock": {
          "type": "date"
        }
      }
    }
  }
}
